//Problem link - https://leetcode.com/problems/evaluate-reverse-polish-notation/

soln:- 
 
 int evalRPN(vector<string>& tokens) {
        stack<long> st;
        for(int i=0;i<tokens.size();i++){
            if(tokens[i]=="+" || tokens[i]=="-"|| tokens[i]=="*" || tokens[i]=="/"){
                if(st.size()>1){
                    long tmp = st.top();
                    st.pop();
                    long t2 = st.top();
                    st.pop();
                    if(tokens[i]=="+")
                        tmp+=t2;
                    if(tokens[i]=="-")
                        tmp=t2-tmp;
                    if(tokens[i]=="*")
                        tmp*=t2;
                    if(tokens[i]=="/")
                        tmp = t2/tmp;
                    st.push(tmp);
                    cout<<tmp<<" ";
                }
            }
            else
                st.push(stoi(tokens[i]));
        }
        return (int)st.top();
    }
